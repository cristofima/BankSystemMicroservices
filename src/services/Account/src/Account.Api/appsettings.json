{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MassTransit": {
    "Outbox": {
      "QueryDelaySeconds": 1,
      "DuplicateDetectionWindowMinutes": 5,
      "DisableInboxCleanupService": false
    }
  }
}