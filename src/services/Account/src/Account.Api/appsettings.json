{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MassTransit": {
    "AzureServiceBus": {
      "ConnectionString": "",
      "Transport": {
        "EnablePartitioning": false,
        "RequiresSession": false,
        "MaxDeliveryCount": 5,
        "LockDurationMinutes": 5,
        "DefaultTopicName": "bank-domain-events",
        "DefaultSubscriptionName": "account-service-subscription"
      },
      "Retry": {
        "RetryLimit": 5,
        "InitialRetryIntervalSeconds": 1,
        "MaxRetryIntervalSeconds": 30,
        "RetryIntervalIncrementSeconds": 2
      },
      "Timeout": {
        "RequestTimeoutSeconds": 30,
        "MessageTimeToLiveMinutes": 1440
      },
      "Performance": {
        "PrefetchCount": 32,
        "ConcurrentMessageLimit": 16,
        "MaxConcurrentCalls": 16
      },
      "EnableCircuitBreaker": true,
      "CircuitBreaker": {
        "TripThreshold": 5,
        "ActiveThreshold": 5,
        "ResetIntervalMinutes": 5,
        "TrackingPeriodMinutes": 1
      },
      "Development": {
        "UseInMemoryForDevelopment": false,
        "EnableDetailedLogging": false
      }
    },
    "Outbox": {
      "QueryDelaySeconds": 1,
      "DuplicateDetectionWindowMinutes": 5,
      "DisableInboxCleanupService": true
    }
  }
}