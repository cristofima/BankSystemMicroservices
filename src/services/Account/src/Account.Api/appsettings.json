{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MassTransit": {
    "AzureServiceBus": {
      "ConnectionString": ""
    },
    "Retry": {
      "RetryLimit": 5,
      "InitialRetryIntervalSeconds": 1,
      "MaxRetryIntervalSeconds": 30,
      "RetryIntervalIncrementSeconds": 2
    },
    "CircuitBreaker": {
      "TripThreshold": 5,
      "ActiveThreshold": 5,
      "ResetIntervalMinutes": 5,
      "TrackingPeriodMinutes": 1
    },
    "Outbox": {
      "QueryDelaySeconds": 1,
      "DuplicateDetectionWindowMinutes": 5,
      "DisableInboxCleanupService": false
    }
  }
}
